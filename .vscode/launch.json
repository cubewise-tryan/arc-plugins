   "version": "0.2.0",
   "configurations": [
      {
         "type": "chrome",
         "request": "launch",
         "name": "Launch Chrome against localhost, with sourcemaps",
         "url": "http://192.168.1.4:7070/#/",
         "webRoot": "${workspaceFolder}"
      },
      {
         "type": "chrome",
         "request": "attach",
         "name": "Attach to Chrome, with sourcemaps",
         "port": 9222,
         "webRoot": "${workspaceFolder}"
      }
   ]
}
